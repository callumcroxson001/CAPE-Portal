
@{ ViewBag.Title = "ViewList"; }

<div class="col-md-12">
    <h2>@ViewBag.List list</h2>
    <a href="javascript:window.print();">Print/Export Report</a>
</div>
<table class="table">
    <tr>
        <th>ID:</th>
        <th>Name:</th>
    </tr>
    @foreach (var i in Model)
    {
<tr>
    <th>@i.ID</th>
    <th>@i.name</th>
</tr>}
</table>

